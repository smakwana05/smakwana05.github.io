{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@img/*": ["/src/images/*"],
      "@pub/*":["src/public/*"],
      "@layouts/*":["src/layouts/*"],
    }
  }
}