---
import HtmlBody from "../layouts/HtmlBody.astro";
import Footer from "../components/Footer.astro";
import Header from "../components/Header.astro";
import Preloader from "../components/Preloader.astro";
import MainImgButton from "../components/MainImgButton.astro";
---



<HtmlBody>
<Preloader/>

<main class="allcontent">
	<Header/>

	<div id="swup" class="gridwrapper">
		<div class="container">
			<MainImgButton
				imagePath="/src/images/Hand-with-Reflecting-Sphere.jpg"
				altText="testin"
				url="/art/life-drawing"
				buttontext="ART"
				theme="art"
			/>
			<MainImgButton
				imagePath="/src/images/Lion.jpg"
				altText="testin"
				url="/architecture"
				buttontext="ART 2"
				theme="art"
			/>
			<MainImgButton
				imagePath="/src/images/magritte-sketch.jpg"
				altText="testin"
				url="/travel"
				buttontext="ART 3"
				theme="art"
			/>
		</div>
		<Footer/>
	</div>
</main>

</HtmlBody>